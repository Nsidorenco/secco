% gcd(a,b) %
(
    a := input();
    b := input();
    if a > b then max := a else max := b;
    while b != 0 do
        (
            c := b;

            i := a;
            while i > 0 do
                i := i - b;

            if i < 0 then
                i := i + b
            else
                i;

            b := i; 
            a := c
        );
    if max < a+b then error() else a+b
)
